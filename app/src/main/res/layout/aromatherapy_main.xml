<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.healingtime.healingtime.MainActivity"
    android:background="#F4F3D7">

    <RelativeLayout
        android:id="@+id/aroma_therapy_top"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#FFFFFF">
        <ImageButton
            android:id="@+id/move_to_main"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:src="@drawable/main_top_img1"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            android:background="#ffffff"/>
        <ImageButton
            android:id="@+id/move_to_search"
            android:layout_width="wrap_content"
            android:layout_height="21dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:src="@drawable/search_green"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            android:background="#ffffff"
            tools:ignore="RtlHardcoded" />
        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="#CDCFCA"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/aroma_therapy_status"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:layout_below="@id/aroma_therapy_top">
        <ImageView
            android:id="@+id/aroma_status_image_a"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="#8C8C8C"
            android:scaleType="centerCrop"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_above="@+id/aroma_therapy_status_line">
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_margin="20dp"
                android:src="@drawable/left_slide"
                android:background="#00000000"/>
            <TextView
                android:id="@+id/aroma_status_text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="설정값 없음"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="#FFFFFF"
                android:shadowColor="#000000"
                android:shadowRadius="5"
                android:shadowDx="1"
                android:shadowDy="1"/>
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_margin="20dp"
                android:src="@drawable/right_slide"
                android:background="#00000000"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/aroma_therapy_status_line"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/aroma_status_image_a"
            android:background="#6A000000">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_margin="10dp"
                android:text="  "
                android:textStyle="bold"/>
            <TextView
                android:id="@+id/main_status_text_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="10dp"
                android:text="  "
                android:textStyle="bold"/>
        </RelativeLayout>
    </RelativeLayout>

    <ScrollView
        android:id="@+id/aroma_therapy_body"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/aroma_therapy_status"
        android:layout_above="@+id/aroma_therapy_bottom">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/aroma_therapy_aroma_sector"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/aroma_boxes1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">
                    <RelativeLayout
                        android:id="@+id/aroma_box1"
                        android:layout_width="110dp"
                        android:layout_height="95dp"
                        android:layout_marginLeft="5dp"
                        android:layout_alignParentLeft="true">
                        <ImageView
                            android:id="@+id/aroma_box1_image"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:adjustViewBounds="true"
                            android:scaleType="fitXY"
                            android:background="@drawable/aromatherapy_round_btn_gray"/>
                        <TextView
                            android:id="@+id/aroma_box1_text1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="18dp"
                            android:text="A구간"
                            android:textSize="19dp"
                            android:textStyle="bold"
                            android:textColor="#FFFFFF"
                            android:shadowColor="#000000"
                            android:shadowRadius="5"
                            android:shadowDx="1"
                            android:shadowDy="1" />
                        <TextView
                            android:id="@+id/aroma_box1_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/aroma_box1_text1"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="5dp"
                            android:text="설정값 없음"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            android:textColor="#FFFFFF"
                            android:shadowColor="#000000"
                            android:shadowRadius="5"
                            android:shadowDx="1"
                            android:shadowDy="1" />
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/aroma_box2"
                        android:layout_width="110dp"
                        android:layout_height="95dp"
                        android:layout_centerInParent="true">
                        <ImageView
                            android:id="@+id/aroma_box2_image"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:adjustViewBounds="true"
                            android:scaleType="fitXY"
                            android:background="@drawable/aromatherapy_round_btn_gray"/>
                        <TextView
                            android:id="@+id/aroma_box2_text1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="18dp"
                            android:text="B구간"
                            android:textSize="19dp"
                            android:textStyle="bold"
                            android:textColor="#FFFFFF"
                            android:shadowColor="#000000"
                            android:shadowRadius="5"
                            android:shadowDx="1"
                            android:shadowDy="1" />
                        <TextView
                            android:id="@+id/aroma_box2_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/aroma_box2_text1"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="5dp"
                            android:text="설정값 없음"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            android:textColor="#FFFFFF"
                            android:shadowColor="#000000"
                            android:shadowRadius="5"
                            android:shadowDx="1"
                            android:shadowDy="1" />
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/aroma_box3"
                        android:layout_width="110dp"
                        android:layout_height="95dp"
                        android:layout_marginRight="5dp"
                        android:layout_alignParentRight="true">
                        <ImageView
                            android:id="@+id/aroma_box3_image"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:adjustViewBounds="true"
                            android:scaleType="fitXY"
                            android:background="@drawable/aromatherapy_round_btn_gray"/>
                        <TextView
                            android:id="@+id/aroma_box3_text1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="18dp"
                            android:text="C구간"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            android:textColor="#FFFFFF"
                            android:shadowColor="#000000"
                            android:shadowRadius="5"
                            android:shadowDx="1"
                            android:shadowDy="1" />
                        <TextView
                            android:id="@+id/aroma_box3_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/aroma_box3_text1"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="5dp"
                            android:text="설정값 없음"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            android:textColor="#FFFFFF"
                            android:shadowColor="#000000"
                            android:shadowRadius="5"
                            android:shadowDx="1"
                            android:shadowDy="1" />/>
                    </RelativeLayout>

                </RelativeLayout>
            </RelativeLayout>

            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#94C84C"/>
            <RelativeLayout
                android:id="@+id/aroma_therapy_aroma_set"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="6dp"
                android:layout_below="@id/aroma_therapy_aroma_sector">
                <ImageView
                    android:id="@+id/run_time_set_image"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="20dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/aroma_green"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/run_time_set_image"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:text="향기설정"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="#94C84C"/>
                <ImageButton
                    android:id="@+id/aroma_therapy_aroma_set_btn"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/next_page"
                    android:background="@drawable/round_btn_run_time_set"
                    android:scaleType="fitXY"/>
            </RelativeLayout>
            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_below="@+id/aroma_therapy_aroma_set"
                android:background="#94C84C"/>
            <RelativeLayout
                android:id="@+id/aroma_remaining_amount"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="6dp"
                android:layout_below="@+id/aroma_therapy_aroma_set">
                <ImageView
                    android:id="@+id/aroma_remaining_amount_image"
                    android:layout_width="27dp"
                    android:layout_height="27dp"
                    android:layout_marginLeft="20dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/remaining_amount"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/aroma_remaining_amount_image"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:text="잔여량"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="#94C84C"/>
                <ImageButton
                    android:id="@+id/aroma_therapy_aroma_remaining_btn"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/next_page"
                    android:background="@drawable/round_btn_run_time_set"
                    android:scaleType="fitXY"/>
            </RelativeLayout>
            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_below="@+id/aroma_remaining_amount"
                android:background="#94C84C"/>

            <RelativeLayout
                android:id="@+id/aroma_spray_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_below="@id/aroma_remaining_amount">
                <ImageButton
                    android:id="@+id/aroma_therapy_spray_btn"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_centerInParent="true"
                    android:src="@drawable/spray_button"
                    android:scaleType="fitXY"
                    android:background="#00000000"/>
            </RelativeLayout>

            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#94C84C"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="1dp"
                android:layout_below="@id/aroma_spray_btn">
                <ImageView
                    android:id="@+id/run_time_set_image2"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_marginLeft="20dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/run_time_set"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/run_time_set_image2"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:text="작동시간설정"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="#7A7878"/>
                <ImageButton
                    android:id="@+id/aroma_therapy_aroma_timeset_btn"
                    android:layout_width="37dp"
                    android:layout_height="37dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/run_time_add"
                    android:background="@drawable/round_btn_run_time_set"
                    android:scaleType="fitXY"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/aroma_therapy_setting_value_a"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="20dp"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentLeft="true"
                    android:orientation="vertical">
                    <View
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_marginRight="10dp"
                        android:layout_below="@+id/aroma_digest"
                        android:background="#CDCFCA"/>
                    <TextView
                        android:id="@+id/aroma_therapy_setting_a_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="3dp"
                        android:text="시작시간 설정안된 ~ 종료시간 설정안됨"
                        android:textColor="#817F73"
                        android:textSize="14dp"/>
                    <TextView
                        android:id="@+id/aroma_therapy_setting_a_aroma"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="3dp"
                        android:text="향기이름: 설정안됨"
                        android:textColor="#817F73"
                        android:textSize="14dp"/>
                    <TextView
                        android:id="@+id/aroma_therapy_setting_a_repeat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="3dp"
                        android:text="반복: 설정안됨    주기: 설정안됨"
                        android:textColor="#817F73"
                        android:textSize="14dp"/>
                </LinearLayout>
                <ImageButton
                    android:id="@+id/aroma_a_onoff"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/run_time_on"
                    android:scaleType="fitCenter"
                    android:background="#00000000"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/aroma_therapy_setting_value_b"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="20dp"
                android:visibility="gone">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentLeft="true"
                    android:orientation="vertical">
                    <View
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_marginRight="10dp"
                        android:layout_below="@+id/aroma_digest"
                        android:background="#CDCFCA"/>
                    <TextView
                        android:id="@+id/aroma_therapy_setting_b_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="3dp"
                        android:text="시작시간 설정안된 ~ 종료시간 설정안됨"
                        android:textColor="#817F73"
                        android:textSize="14dp"/>
                    <TextView
                        android:id="@+id/aroma_therapy_setting_b_aroma"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="3dp"
                        android:text="향기이름: 설정안됨"
                        android:textColor="#817F73"
                        android:textSize="14dp"/>
                    <TextView
                        android:id="@+id/aroma_therapy_setting_b_repeat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="3dp"
                        android:text="반복: 설정안됨    주기: 설정안됨"
                        android:textColor="#817F73"
                        android:textSize="14dp"/>
                </LinearLayout>
                <ImageButton
                    android:id="@+id/aroma_b_onoff"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/run_time_on"
                    android:scaleType="fitCenter"
                    android:background="#00000000"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/aroma_therapy_setting_value_c"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="20dp"
                android:visibility="gone">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentLeft="true"
                    android:orientation="vertical">
                    <View
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_marginRight="10dp"
                        android:layout_below="@+id/aroma_digest"
                        android:background="#CDCFCA"/>
                    <TextView
                        android:id="@+id/aroma_therapy_setting_c_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="3dp"
                        android:text="시작시간 설정안된 ~ 종료시간 설정안됨"
                        android:textColor="#817F73"
                        android:textSize="14dp"/>
                    <TextView
                        android:id="@+id/aroma_therapy_setting_c_aroma"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="3dp"
                        android:text="향기이름: 설정안됨"
                        android:textColor="#817F73"
                        android:textSize="14dp"/>
                    <TextView
                        android:id="@+id/aroma_therapy_setting_c_repeat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="3dp"
                        android:text="반복: 설정안됨    주기: 설정안됨"
                        android:textColor="#817F73"
                        android:textSize="14dp"/>
                </LinearLayout>
                <ImageButton
                    android:id="@+id/aroma_c_onoff"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/run_time_on"
                    android:scaleType="fitCenter"
                    android:background="#00000000"/>
            </RelativeLayout>
            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="20dp"
                android:layout_below="@+id/aroma_remaining_amount"
                android:background="#94C84C"/>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/aroma_therapy_bottom"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:layout_weight="1"
            android:background="@drawable/setting_bottom_green">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_gravity="center"
                android:src="@drawable/bright1"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:text="칼라 테라피"
                android:textColor="#76BB17"
                android:textStyle="bold"
                android:textSize="12dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:layout_weight="1"
            android:background="@drawable/setting_bottom_white">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_gravity="center"
                android:src="@drawable/aroma_therapy"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:text="아로마 테라피"
                android:textColor="#76BB17"
                android:textStyle="bold"
                android:textSize="12dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:layout_weight="1"
            android:background="@drawable/setting_bottom_green">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_gravity="center"
                android:src="@drawable/settings"/>
            <TextView
